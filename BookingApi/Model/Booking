using System;
namespece BookingAPIModels
{
    public class Booking
    {
        [Key]
        public int Uuid {get; set;}

        public string Email {get; set;}

        public struct DateTime Datetime {get; set;}

        public bool Isbooked {get; set;}
    }

    public Booking()
    {

    }

    public Booking(int uuid, string email, struct DateTime dateTime, bool isbooked)
    {
        Uuid = uuid;
        Email = email;
        DateTime = dateTime;
        Isbooked = isbooked;
    }

    public override string ToString()
    {
        return $"Uuid: {Uuid} | Email: {Email} | DateTime: {DateTime} | Isbooked = {Isbooked}";
        
    }
}

